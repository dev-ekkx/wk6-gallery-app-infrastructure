{
  "family": "gallery-task",
  "executionRoleArn": "arn:aws:iam::124355655853:role/gallery-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::124355655853:role/gallery-ecs-task-execution-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "gallery-v2-container",
      "image": "124355655853.dkr.ecr.eu-west-1.amazonaws.com/wk6-gallery-app:latest",
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 8081,
          "protocol": "http"
        }
      ],
      "environment": [
        {
            "name": "AWS_REGION",
            "value": "eu-west-1"
        },
        {
            "name": "S3_BUCKET_NAME",
            "value": "week6-image-upload"
        },
        {
            "name": "PORT",
            "value": "8080"
        },
        {
          "name": "DB_HOST",
          "value": "gallery-v2-db.cn8aem2220t7.eu-west-1.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "DB_NAME",
          "value": "galleryV2Db"
        },
        {
            "name": "GIN_MODE",
            "value": "release"
        }
      ],
      "secrets": [
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:ssm:eu-west-1:124355655853:parameter/gallery-v2/db/username"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:eu-west-1:124355655853:parameter/gallery-v2/db/password"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/gallery-v2",
          "awslogs-region": "eu-west-1",
          "awslogs-create-group": "true",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8081 || exit 1"
        ],
        "interval": 30,
        "timeout": 10,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}